{
  "name": "tada-list",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "vercel-build": "yarn generate:prisma && prisma migrate deploy && yarn build",
    "start": "next start",
    "lint": "next lint",
    "db:push": "dotenv -e .env.local -- prisma db push",
    "db:seed": "dotenv -e .env.local -- prisma db seed --preview-feature",
    "db:pull": "dotenv -e .env.local -- prisma db pull",
    "studio": "dotenv -e .env.local -- prisma studio",
    "generate": "yarn generate:prisma && yarn generate:nexus",
    "generate:nexus": "ts-node --compiler-options '{\"module\":\"CommonJS\"}' --transpile-only graphql/schema",
    "generate:prisma": "prisma generate",
    "clean": "rm -rf ./node_modules ./.next && yarn"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@apollo/client": "^3.5.6",
    "@prisma/client": "^3.7.0",
    "apollo-server-micro": "^3.6.1",
    "graphql": "^15.3.0",
    "graphql-scalars": "^1.14.1",
    "micro": "^9.3.4",
    "micro-cors": "^0.1.1",
    "next": "12.0.7",
    "nexus": "^1.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@types/micro-cors": "^0.1.2",
    "@types/node": "17.0.8",
    "@types/react": "17.0.38",
    "dotenv-cli": "^4.1.1",
    "eslint": "8.6.0",
    "eslint-config-next": "12.0.7",
    "prisma": "^3.7.0",
    "ts-node": "^10.4.0",
    "typescript": "4.5.4"
  }
}
